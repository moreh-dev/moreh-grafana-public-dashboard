          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "SELECT sum(device_count) as AvailableDevices\r\nFROM ( SELECT \r\n    Backend.id as be_id, count(Device.id) as device_count\r\n  FROM Device\r\n  LEFT JOIN DeviceStatus ON Device.stat_id = DeviceStatus.id\r\n  LEFT JOIN Backend on Device.be_id = Backend.id\r\n  LEFT JOIN BackendStatus ON BackendStatus.id = Backend.stat_id\r\n  LEFT JOIN BackendGroup ON Backend.group_id = BackendGroup.id\r\n  WHERE\r\n    BackendStatus.status in (\"ACTIVE\") and DeviceStatus.status = \"IDLE\" and BackendGroup.name in ($BackendGroup)\r\n  GROUP BY Backend.id\r\n) AS A",
          "refId": "C",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {

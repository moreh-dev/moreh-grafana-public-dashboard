        {
          "dataset": "moreh_master",
          "datasource": {
            "type": "mysql",
            "uid": "${DS_SDAMANAGER}"
          },
          "editorMode": "code",
          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": " SELECT \r\n    coalesce(count(distinct Device.id), 0) as RunningDevices\r\n  FROM Device\r\n  LEFT JOIN DeviceStatus ON Device.stat_id = DeviceStatus.id\r\n  LEFT JOIN Backend on Device.be_id = Backend.id\r\n  LEFT JOIN BackendStatus ON BackendStatus.id = Backend.stat_id\r\n  LEFT JOIN BackendGroup ON Backend.group_id = BackendGroup.id\r\n  WHERE\r\n    BackendStatus.status in (\"ACTIVE\") and DeviceStatus.status = \"PROCESSING\" and BackendGroup.name in ($BackendGroup)",
          "refId": "B",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
